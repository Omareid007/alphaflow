entrypoint = "server/index.ts"
modules = ["nodejs-22", "postgresql-16"]
hidden = [".next", "node_modules", ".cache", "server_dist"]
run = "npm run dev"

[languages]
[languages.typescript]
pattern = "**/*.{ts,tsx}"
syntax = "typescript"

[languages.javascript]
pattern = "**/*.{js,jsx}"
syntax = "javascript"

[nix]
channel = "stable-24_05"
packages = ["jq"]

[deployment]
deploymentTarget = "cloudrun"
build = ["sh", "-c", "npm run build"]
run = ["npm", "run", "start"]

[[ports]]
localPort = 3000
externalPort = 80

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 3002
externalPort = 3002

[[ports]]
localPort = 3003
externalPort = 3003

[[ports]]
localPort = 3004
externalPort = 3000

[[ports]]
localPort = 5000
externalPort = 5000

[web]
open = true
port = 3000

[env]
PORT = "5000"
NODE_OPTIONS = "--max-old-space-size=768"
NEXT_TELEMETRY_DISABLED = "1"

[agent]
stack = "NEXTJS"
integrations = ["javascript_openai_ai_integrations:1.0.0", "javascript_database:1.0.0", "javascript_anthropic_ai_integrations:2.0.0", "javascript_openrouter_ai_integrations:2.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start dev servers"

[[workflows.workflow]]
name = "Start dev servers"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 3000

